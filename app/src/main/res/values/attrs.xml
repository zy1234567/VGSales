<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TopBar" >
        <attr name="barTitle" format="string" />
        <attr name="srcLeft" format="reference" />
        <attr name="srcRight" format="reference" />
        <attr name="showBackNav" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="drawBorder" format="boolean" />
        <attr name="borderColor" format="color" />
        <attr name="borderWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleGapView">
        <attr name="color" format="color" />
    </declare-styleable>
</resources>