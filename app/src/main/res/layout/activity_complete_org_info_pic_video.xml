<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_107"
    android:orientation="vertical"
    tools:context="com.ztstech.vgmate.activitys.complete_org_info.subview.pic_video.CompleteOrgInfoPicVideoActivity">

    <com.ztstech.vgmate.weigets.TopBar
        android:background="#fff"
        app:barTitle="图片／视频"
        app:showBackNav="true"
        app:textRight="保存"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.ztstech.vgmate.weigets.TopBar>

    <FrameLayout
        android:paddingTop="15dp"
        android:layout_marginTop="8dp"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.ztstech.vgmate.weigets.CustomGridView
            android:id="@+id/cgv"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </com.ztstech.vgmate.weigets.CustomGridView>

    </FrameLayout>
</LinearLayout>
